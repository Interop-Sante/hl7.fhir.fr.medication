       fhir:requirements [ fhir:v "The developper SHALL support the infusion use case, which requires a multi-compounds Medication ressource." ] ;
       fhir:min [ fhir:v "1"^^xsd:nonNegativeInteger ]
     ] [
       fhir:id [ fhir:v "Medication.code.coding" ] ;
