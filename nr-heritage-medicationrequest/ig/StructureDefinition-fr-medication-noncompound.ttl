       fhir:path [ fhir:v "Medication.ingredient.item[x]" ] ;
       fhir:short [ fhir:v "Substance" ] ;
       fhir:definition [ fhir:v "The actual substance (simple ingredient) of the simple medication (ie. made of single component)." ] ;
       fhir:requirements [ fhir:v "The composition of the medication for this ingredient SHALL be defined for prescrption of virtual medication so that strength can be defined. It MAY be defined for other medication" ] ;
       fhir:type ( [
         fhir:code [ fhir:v "CodeableConcept"^^xsd:anyURI ] ;
         fhir:profile ( [
           fhir:v "https://hl7.fr/ig/fhir/medication/StructureDefinition/fr-mp-substance"^^xsd:anyURI ;
           fhir:link <https://hl7.fr/ig/fhir/medication/StructureDefinition/fr-mp-substance>
         ] )
       ] )
     ] [
       fhir:id [ fhir:v "Medication.ingredient.strength" ] ;
       fhir:path [ fhir:v "Medication.ingredient.strength" ] ;
       fhir:definition [ fhir:v "Specifies how much of the items there are in this Medication" ] ;
       fhir:comment [ fhir:v "The quantity of this igredient defining the medication. For instance, 50 mg/mL defining the glucose quantity for the glucose 5% or 500 mg medicine and 30 mg respectively defining the paracetamol and the codeine quantity for the paracetamol+codeine 500 mg+30 mg medicine." ] ;
       fhir:requirements [ fhir:v "In cas of virtual medication strength SHALL be provided" ] ;
       fhir:type ( [
         fhir:code [ fhir:v "Ratio"^^xsd:anyURI ] ;
         fhir:profile ( [
           fhir:v "https://hl7.fr/ig/fhir/medication/StructureDefinition/FrRatioMedication"^^xsd:anyURI ;
           fhir:link <https://hl7.fr/ig/fhir/medication/StructureDefinition/FrRatioMedication>
         ] )
       ] )
     ] [
       fhir:id [ fhir:v "Medication.batch" ] ;
       fhir:path [ fhir:v "Medication.batch" ] ;
       fhir:definition [ fhir:v "This applies for the proprietary named medication involved in a MedicationDispense, MedicationAdministration or a MedicationUse." ] ;
       fhir:comment [ fhir:v "WARNING: it is the batch number of the part.\r\nIf it is not discerned for each part, it SHALL NOT be provided here but at the grouping level where it is defined, most often the UCD." ]
     ] )
  ] . # 

