           fhir:type [ fhir:v "pattern" ] ;
           fhir:path [ fhir:v "coding.system" ]
         ] ) ;
         fhir:description [ fhir:v "One slice for code from the official medication referential in France: Référentiel Unique d'Interopérabilité du Médicament (RUIM) browsable with https://smt.esante.gouv.fr/explorer-les-concepts/terminologie-ref_interop_med/ and downloadable through https://smt.esante.gouv.fr/catalogue-des-terminologies/" ] ;
         fhir:rules [ fhir:v "open" ]
       ] ;
       fhir:min [ fhir:v "1"^^xsd:nonNegativeInteger ]
     ] [
       fhir:id [ fhir:v "Medication.code.coding:nonCompoundCode" ] ;
       fhir:path [ fhir:v "Medication.code.coding" ] ;
       fhir:sliceName [ fhir:v "nonCompoundCode" ] ;
       fhir:short [ fhir:v "Codes that identify this simple medication" ] ;
       fhir:definition [ fhir:v "Codes that identify this simple medication" ] ;
       fhir:min [ fhir:v "1"^^xsd:nonNegativeInteger ] ;
       fhir:max [ fhir:v "1" ] ;
       fhir:mustSupport [ fhir:v true ] ;
       fhir:binding [
         fhir:strength [ fhir:v "required" ] ;
         fhir:description [ fhir:v "UCD, SMS or ANSM code" ] ;
         fhir:valueSet [
           fhir:v "https://hl7.fr/ig/fhir/medication/ValueSet/fr-medication-code"^^xsd:anyURI ;
           fhir:link <https://hl7.fr/ig/fhir/medication/ValueSet/fr-medication-code>
         ]
       ]
     ] [
       fhir:id [ fhir:v "Medication.code.text" ] ;
       fhir:path [ fhir:v "Medication.code.text" ] ;
       fhir:comment [ fhir:v "When some code is available, the text is the same as a displayName of one of the codings." ]
     ] [
       fhir:id [ fhir:v "Medication.form" ] ;
       fhir:path [ fhir:v "Medication.form" ] ;
       fhir:comment [ fhir:v "When branded medication is referenced from MedicationRequest, this is the ordered form.  When Medication is referenced within MedicationDispense, this is the dispensed form.  When branded medication is referenced within MedicationAdministration, this is administered form." ] ;
       fhir:binding [
         fhir:strength [ fhir:v "extensible" ] ;
         fhir:valueSet [
           fhir:v "https://hl7.fr/ig/fhir/medication/ValueSet/fr-mp-dose-form"^^xsd:anyURI ;
           fhir:link <https://hl7.fr/ig/fhir/medication/ValueSet/fr-mp-dose-form>
         ]
       ]
     ] [
       fhir:id [ fhir:v "Medication.amount" ] ;
       fhir:path [ fhir:v "Medication.amount" ] ;
       fhir:comment [ fhir:v "If the strenght of the ingredients is a concentration, it is the volume (liquid) or mass (ointment) in the Medication of the MedicationRequest, Dispense or Use (pka MedicationStatement).\r\nIf there is only one ingredient and the strenght is not defined, it is the quantity of the substance." ]
     ] [
       fhir:id [ fhir:v "Medication.ingredient" ] ;
