  fhir:differential [
     fhir:element ( [
       fhir:id [ fhir:v "Medication" ] ;
       fhir:path [ fhir:v "Medication" ] ;
       fhir:short [ fhir:v "Simple Medication" ] ;
       fhir:definition [ fhir:v "This ressource is profiled for describing a simple medication (vs compound medication) in the presription line represented by a MedicationRequest, a MedicationDispense or a MedicationUsage (pka MedicationStatement)." ] ;
       fhir:comment [ fhir:v "A simple medication is made of one single component made of one to many substances.\r\nOptionality of doseForm, amount and strehgth attributes supports different levels of granularity.\r\nThe coarser level only defines the substances it's made of. For instance, contaning only paracetamol or containing only glucose.\r\nThe finest level is the defines the composition and the dose form. For instance, paracetamol 500 mg tablet or  500 mL glucose 5% (50 mg/mL) solution for injection." ] ;
       fhir:constraint ( [
         fhir:key [ fhir:v "frmed-med-1" ] ;
         fhir:severity [ fhir:v "error" ] ;
         fhir:human [ fhir:v "If fr-medication is a simple medication, ingredient SHALL NOT be a Reference" ] ;
         fhir:expression [ fhir:v "code.coding.system.exists() implies (ingredient.item as Reference).empty()" ] ;
         fhir:source [
           fhir:v "https://hl7.fr/ig/fhir/medication/StructureDefinition/fr-medication-noncompound"^^xsd:anyURI ;
           fhir:link <https://hl7.fr/ig/fhir/medication/StructureDefinition/fr-medication-noncompound>
         ]
       ] )
     ] [
       fhir:id [ fhir:v "Medication.extension" ] ;
       fhir:path [ fhir:v "Medication.extension" ] ;
       fhir:slicing [
         fhir:discriminator ( [
           fhir:type [ fhir:v "value" ] ;
           fhir:path [ fhir:v "url" ]
         ] ) ;
         fhir:rules [ fhir:v "open" ]
       ]
     ] [
       fhir:id [ fhir:v "Medication.extension:drugCharacteristic" ] ;
       fhir:path [ fhir:v "Medication.extension" ] ;
       fhir:sliceName [ fhir:v "drugCharacteristic" ] ;
       fhir:short [ fhir:v "descriptive properties of the medicine" ] ;
       fhir:min [ fhir:v "0"^^xsd:nonNegativeInteger ] ;
       fhir:max [ fhir:v "*" ] ;
       fhir:type ( [
         fhir:code [ fhir:v "Extension"^^xsd:anyURI ] ;
         fhir:profile ( [
           fhir:v "https://hl7.fr/ig/fhir/medication/StructureDefinition/fr-drug-characteristic"^^xsd:anyURI ;
           fhir:link <https://hl7.fr/ig/fhir/medication/StructureDefinition/fr-drug-characteristic>
         ] )
       ] )
     ] [
       fhir:id [ fhir:v "Medication.code" ] ;
       fhir:path [ fhir:v "Medication.code" ] ;
