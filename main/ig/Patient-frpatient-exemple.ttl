# - resource -------------------------------------------------------------------

<http://hl7.fr/fhir/fr/example/Patient/frpatient-exemple> a fhir:Patient ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "frpatient-exemple"] ; # 
  fhir:meta [
     fhir:profile ( [
       fhir:v "http://hl7.fr/fhir/fr/example/StructureDefinition/fr-patient"^^xsd:anyURI ;
       fhir:link <http://hl7.fr/fhir/fr/example/StructureDefinition/fr-patient>
     ] )
  ] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\"><p style=\"border: 1px #661aff solid; background-color: #e6e6ff; padding: 10px;\"><b>Anonymous Patient</b> male, DoB Unknown</p><hr/></div>"
  ] ; # 
  fhir:gender [ fhir:v "male"] . # 

